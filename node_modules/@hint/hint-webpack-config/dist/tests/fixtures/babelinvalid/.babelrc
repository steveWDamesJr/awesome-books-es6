{
    "presets": [
      ["env", {
        "targets": {
          "browsers": ["last 2 versions", "> 5% in BE"],
          "uglify": true
        },
        "modules": "commonjs"
      }]
    ],
    "plugins": [
      "syntax-dynamic-import",
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": [
          ["env", {
            "targets": {
              "node": "current"
            }
          }]
        ]
      }
    }
  }
